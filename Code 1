#include<stdio.h>
#include<string.h>
#include<math.h>
void pow(int n);
int main()
{
	printf("\n\t\tWith Argument Without Return Type\n");
	int n;
	printf("\n\t\t    Enter the value for n : ");
	scanf("%d",&n);
	pow(n);
	return 0;
}
void pow(int n)
{
	int i,sum=0;
		for(i=0;i<=n;i++)
	{
		sum=sum+pow(i,3);	
	}
	printf("\n\tSum of the cubes of 1st %d Natural numbers is %d\n\n",n,sum);
}
