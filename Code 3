#include<stdio.h>
#include<string.h>
#include<math.h>
int pow();
int main()
{
	printf("\n\t\tWithout Argument With Return Type\n");
	int sum=pow();
	printf("\n\t  Sum of the cubes of 1st n Natural numbers is %d\n\n",sum);
	return 0;	
}
int pow()
{
	int n;
	printf("\n\t\t     Enter the value for n : ");
	scanf("%d",&n);
	int i,sum=0;
		for(i=0;i<=n;i++)
	{
		sum=sum+pow(i,3);	
	}
	return sum;
}
